{
    "name": "ild78/lib-php",
    "description": "Ild78 PHP library",
    "version": "0.0.7",
    "type": "library",
    "keywords": [
        "payments",
        "payment solution",
        "card",
        "sepa"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ild78\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ild78\\PHPStan\\": "tests/stan",
            "ild78\\Stub\\": "tests/Stub",
            "ild78\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "atoum/atoum": "^3.4",
        "atoum/autoloop-extension": "^0.3.0",
        "atoum/json-schema-extension": "^2.1",
        "atoum/reports-extension": "^3.0",
        "atoum/stubs": "^2.6",
        "atoum/visibility-extension": "^1.3",
        "fakerphp/faker": "^1.13",
        "guzzlehttp/guzzle": "^7.2",
        "jdslv/atoum-report-cobertura": "^1.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.77",
        "phpstan/phpstan-deprecation-rules": "^0.12.5",
        "phpstan/phpstan-strict-rules": "^0.12.5",
        "ramsey/uuid": "^4.1",
        "satesh/phpcs-gitlab-report": "^1.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "suggest": {
        "monolog/monolog": "Allows to log interaction with the API"
    },
    "config": {
        "sort-packages": true
    }
}
